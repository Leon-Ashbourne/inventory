<html>
    <body>
        <a href="/">Back to home</a>
        <h1><%= title %></h1>
        <% if(locals.categories) {%>
                <% categories.forEach((category) => { %> 
                    <h2><%= category %></h2>
                    <%- include("categoryValues", {category: category})%>
                <%})%>
        <% } else {%>
            <h2>No categories to be found...</h2>
        <%}%>
    </body>
</html>