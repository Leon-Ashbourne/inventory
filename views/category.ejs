<html>
    <body>
        <h1><%= title %></h1>
        <% if(categories.length > 0) {%>
            <ul>
                <% categories.forEach((category) => { %> 
                    <li><a href= <%= "/category/"+ category.category %> > <%= category.category %> </a></li>
                <%})%>
            </ul>
        <% } else {%>
            <h2>No categories to be found...</h2>
        <%}%>

    </body>
</html>